{
  "name": "go-extension-pack",
  "displayName": "Go Extension Pack",
  "description": "Popular Visual Studio Code extensions for Go Development",
  "version": "0.9.0",
  "publisher": "doggy8088",
  "icon": "icon.png",
  "preview": true,
  "repository": "https://github.com/doggy8088/go-extension-pack",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Extension Packs",
    "Snippets"
